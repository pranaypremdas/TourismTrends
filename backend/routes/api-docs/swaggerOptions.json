	"servers": [
		{
			"url": "/"
		}
	],
	"tags": [
		{
			"name": "Trends",
			"description": "API queries based tourism trend data."
		},
		{
			"name": "User Data",
			"description": "API queries based on user data."
		},
		{
			"name": "Authentication",
			"description": "User login and registration."
		}
