								}
							}
						}
					},
					"401": {
						"description": "Unauthorized. Invalid email or password.",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"error": {
											"type": "string"
										},
										"message": {
											"type": "string"
										}
									}
								},
								"example": {
									"error": true,
									"message": "Incorrect email or password"
								}
							}
						}
					},
					"500": {
						"description": "Internal server error",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"error": {
											"type": "string"
										}
									}
								},
								"example": {
									"error": true,
									"message": "Internal server error"
								}
							}
						}
					}
				}
			}
		}
	},
	"components": {
