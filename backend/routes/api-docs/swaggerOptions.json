	"paths": {
		"/trends": {
			"get": {
				"tags": ["Trends"],
				"description": "Returns a list of trend data. The list is arranged by date, in ascending order.",
				"operationId": "listAllMatchingTrends",
				"produces": ["application/json"],
				"parameters": [
					{
						"name": "region",
						"in": "body",
						"required": false,
						"description": "Local Government Area by id in array. Defaults to all regions if not provided for Local Government Memebers, defaults to the region of the user for Business Members.",
						"schema": {
							"type": "array"
						}
					},
					{
						"name": "type",
						"in": "body",
						"required": false,
						"description": "Type of trend data in array. Options are ['ave_historical_occupancy', 'ave_daily_rate', 'ave_length_stay', 'ave_booking_window']. Defaults to all types if not provided.",
						"schema": {
							"type": "array"
						}
					},
					{
						"name": "dateRange",
						"in": "body",
						"required": false,
						"description": "Date range in the format of an array ['yyyy-mm-dd', 'yyyy-mm-dd']. Defaults to last two years.",
						"schema": {
							"type": "array"
						}
					}
				],
				"responses": {
					"200": {
						"description": "An array of objects containing id, date, lga_id, lga_name, ave_historical_occupany, ave_daily_rate, ave_length_stay, ave_booking_window. An example of one object in the array is shown below.",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"error": {
											"type": "boolean"
										},
										"message": {
											"type": "string"
										},
										"query": {
											"type": "object",
											"properties": {
												"region": {
													"type": "array"
												},
												"type": {
													"type": "array"
												},
												"dateRange": {
													"type": "array"
												}
											}
										},
										"results": {
											"type": "array",
											"items": {
												"type": "object",
												"properties": {
													"id": {
														"type": "number"
													},
													"date": {
														"type": "string"
													},
													"lga_id": {
														"type": "number"
													},
													"lga_name": {
														"type": "string"
													},
													"ave_historical_occupancy": {
														"type": "number"
													},
													"ave_daily_rate": {
														"type": "number"
													},
													"ave_length_stay": {
														"type": "number"
													},
													"ave_booking_window": {
														"type": "number"
													}
												}
											}
										}
									}
								},
								"example": {
									"error": false,
									"message": "Success",
									"query": {
										"region": [1, 2],
										"type": ["ave_historical_occupancy"],
										"dateRange": ["2024-01-01", "2024-01-31"]
									},
									"results": [
										{
											"id": 1461,
											"date": "2024-01-01",
											"lga_id": 1,
											"lga_name": "Cairns",
											"ave_historical_occupancy": 0.7554
										}
									]
								}
							}
						}
					},
					"400": {
						"description": "Invalid type, region or dateRange provided.",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"error": {
											"type": "string"
										}
									}
								},
								"example": {
									"error": "No region provided."
								}
							}
						}
					},
					"500": {
						"description": "Internal server error",
						"content": {
							"application/json": {
								"schema": {
									"type": "object",
									"properties": {
										"error": {
											"type": "string"
										}
									}
								},
								"example": {
									"error": true,
									"message": "Internal server error"
								}
							}
						}
					}
				}
			}
		},
